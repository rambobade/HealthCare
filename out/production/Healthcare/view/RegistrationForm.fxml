      </TextField>
      <TextField fx:id="txtConfirmPassword" layoutX="47.0" layoutY="259.0" onAction="#txtConfirmPasswordOnAction" prefHeight="27.0" prefWidth="252.0" promptText="Confirm Password" style="-fx-background-color: black;">
         <font>
            <Font size="16.0" />
         </font>
      </TextField>
      <Label fx:id="lblpassword1" layoutX="173.0" layoutY="237.0" text="Password do not match" textFill="#f20202" />
      <Label fx:id="lblpassword2" layoutX="173.0" layoutY="296.0" text="Password do not match" textFill="#f20202" />
