            <Font name="System Bold" size="28.0" />
         </font>
      </Label>
      <Separator layoutX="32.0" layoutY="76.0" prefHeight="0.0" prefWidth="282.0" />
      <TextField fx:id="txtFullName" layoutX="47.0" layoutY="99.0" prefHeight="27.0" prefWidth="252.0" promptText="FullName" style="-fx-background-color: black;">
         <font>
            <Font size="16.0" />
         </font>
      </TextField>
      <TextField fx:id="txtMobile" layoutX="47.0" layoutY="148.0" prefHeight="27.0" prefWidth="252.0" promptText="Mobile" style="-fx-background-color: black;">
         <font>
            <Font size="16.0" />
         </font>
      </TextField>
      <Button layoutX="66.0" layoutY="328.0" mnemonicParsing="false" onAction="#btnSignUp" prefHeight="27.0" prefWidth="214.0" style="-fx-background-color: orange;" text="Registration">
         <font>
            <Font name="System Bold" size="13.0" />
         </font>
      </Button>
      <TextField fx:id="txtPassword" layoutX="47.0" layoutY="200.0" prefHeight="27.0" prefWidth="252.0" promptText="Password" style="-fx-background-color: black;">
